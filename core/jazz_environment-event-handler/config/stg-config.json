{
  "BASE_API_URL": "https://{conf-apikey}.execute-api.{conf-region}.amazonaws.com/stg/jazz",
  "ENVIRONMENT_API_RESOURCE": "/environments",
  "DELETE_ENVIRONMENT_API_RESOURCE": "/delete-serverless-service",
  "TOKEN_URL": "/login",
  "SERVICE_API_RESOURCE": "/services",
  "SERVICE_USER": "{jazz_admin}",
  "TOKEN_CREDS": "{jazz_admin_creds}",
  "POLICIES": "/acl/policies",
  "ENV_DETAILS": "/environments",
  "EVENTS": {
    "EVENT_TYPE": [
      "SERVICE_DEPLOYMENT",
      "SERVICE_ONBOARDING",
      "SERVICE_DELETION"
    ],
    "EVENT_NAME": [
      "COMMIT_TEMPLATE",
      "CREATE_BRANCH",
      "COMMIT_CODE",
      "UPDATE_ENVIRONMENT",
      "DELETE_BRANCH",
      "DELETE_ENVIRONMENT"
    ],
    "UPDATE_ENVIRONMENT": "UPDATE_ENVIRONMENT",
    "INITIAL_COMMIT": "COMMIT_TEMPLATE",
    "COMMIT_CODE": "COMMIT_CODE",
    "CREATE_BRANCH": "CREATE_BRANCH",
    "DELETE_BRANCH": "DELETE_BRANCH",
    "DELETE_ENVIRONMENT": "DELETE_ENVIRONMENT"
  },
  "ENVIRONMENT_CREATE_REQUIRED_FIELDS": [
    "domain",
    "branch",
    "created_by",
    "service_name"
  ],
  "ENVIRONMENT_UPDATE_REQUIRED_FIELDS": [
    "domain",
    "service_name"
  ],
  "ENVIRONMENT_PRODUCTION_PHYSICAL_ID": "master",
  "CREATE_ENVIRONMENT_STATUS": "inactive",
  "ENVIRONMENT_DELETE_STARTED_STATUS": "deletion_started",
  "ENVIRONMENT_DELETE_FAILED_STATUS": "deletion_failed",
  "ENVIRONMENT_DELETE_COMPLETED_STATUS": "archived",
  "RANDOM_CHARACTERS": "0123456789abcdefghijklmnopqrstuvwxyz",
  "RANDOM_ID_CHARACTER_COUNT": 10,
  "JENKINS_USER": "{ci_user}",
  "JOB_TOKEN": "{job_token}",
  "API_TOKEN": "{api_token}",
  "JOB_BUILD_URL": "{conf-jenkins-host}/job/",
  "BUILDPACKMAP": {
    "api": "build-pack-api",
    "function": "build-pack-function",
    "website": "build-pack-website",
    "sls-app": "build-pack-sls-app",
    "ui": "jazz_ui"
  },
  "TVAULT": {
    "IS_ENABLED": "{tvault-enabled}",
    "API": "/t-vault/safes",
    "HOSTNAME": "{tvault-host-name}",
    "ADD_ADMINS": "/user",
    "ADD_ROLE": "/role"
  },
  "ADMIN_POLICY": [
    {
      "userId": "",
      "permission": "write",
      "category": "code"
    },
    {
      "userId": "",
      "permission": "admin",
      "category": "manage"
    },
    {
      "userId": "",
      "permission": "write",
      "category": "deploy"
    }
  ]
